<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class entity-name="JobDefinition" name="org.joget.scheduler.model.JobDefinition" table="sch_job_def">
        <id column="id" name="id" type="string"/>
        <property column="name" name="name" type="string"/>
        <property column="appId" name="appId" type="string"/>
        <property column="pluginClass" name="pluginClass" type="string"/>
        <property column="pluginProperties" name="pluginProperties" type="text"/>
        <property column="triggerExp" name="trigger" type="string"/>
        <property column="nextFireTime" name="nextFireTime" type="timestamp"/>
    </class>
</hibernate-mapping>